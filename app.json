{
  "expo": {
    "name": "flavr",
    "slug": "flavr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon/icon.png",
    "scheme": "flavr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lukydemboy.flavr"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/app-icon/android-icon-foreground.png",
        "backgroundImage": "./assets/app-icon/android-icon-background.png",
        "monochromeImage": "./assets/app-icon/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/app-icon/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/app-icon/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to generate recipes from it.",
          "cameraPermission": "The app accesses your camera to take photos of recipes. So we can generate recipes in the app from it.",
          "microphonePermission": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
